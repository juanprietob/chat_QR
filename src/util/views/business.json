{
    "_id": "_design/business",
    "language": "javascript",
    "views": {
        "getWallets": {
            "map": "function(doc) {\n\tif(doc.type == \"business\"){\n\t\t  emit(doc.chat_id, doc.wallet);\t\n\t}\n}"
        },
        "getTransactions": {
            "map": "function(doc) {\n\tif(doc.type == \"transaction\" && doc.transaction){\n\t\t  emit(doc.chat_id, doc.transaction);\t\n\t}\n}"
        }
    }
}